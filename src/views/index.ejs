    <!-- Add Google Maps API Script -->
   <script>
      function initMap() {
        // Map options
        const mapOptions = {
          center: { lat: -12.133434, lng: -76.984369 }, // Example coordinates
          zoom: 19,
        };
        // Create a map object and specify the DOM element for display
        const map = new google.maps.Map(document.getElementById("map"), mapOptions);
      }
      // No necesitas llamar a initMap aquí, Google lo llamará automáticamente
    </script>
    <script
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyArSMWmPN8we93NKGCXkaoDqo07wDApCfg&callback=initMap"
      async
      defer
    ></script>
